        </trans-unit>
        <trans-unit id="VersionText.Text" translate="yes" xml:space="preserve">
          <source>Version: </source>
          <target state="final" state-qualifier="mt-suggestion">Versiune: </target>
        </trans-unit>
        <trans-unit id="WalkingSteps.Text" translate="yes" xml:space="preserve">
          <source>Walking Steps</source>
          <target state="final">Mers normal</target>
        </trans-unit>
        <trans-unit id="Calories.Text" translate="yes" xml:space="preserve">
          <source>Calories</source>
          <target state="final" state-qualifier="mt-suggestion">Calorii</target>
        </trans-unit>
        <trans-unit id="Distance.Text" translate="yes" xml:space="preserve">
          <source>Distance</source>
          <target state="final" state-qualifier="tm-suggestion">Distanța</target>
        </trans-unit>
      </group>
